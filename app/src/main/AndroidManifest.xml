            />
        <activity android:name=".DetailActivity"
            android:label="Detil Produk"

            />
        <activity android:name=".Pemesanan"
            android:label="Pemesanan Anda"
            />
        <activity android:name=".Dunlop"
            android:label="Dunlop Area"

            />
        <activity android:name=".Bridgestone"
            android:label="Bridgestone Area"
            />
        <activity android:name=".MenuBan"
            android:label="Menu Ban"

            />
        <activity android:name=".MerekBan"
